    "postcss-import": "14.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "sass": "1.49.10",
